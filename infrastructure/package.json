{
  "name": "governance-infrastructure",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for Agentic Data Governance Platform",
  "main": "dist/bin/app.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy",
    "deploy:dev": "cdk deploy --context environment=dev",
    "deploy:staging": "cdk deploy --context environment=staging",
    "deploy:prod": "cdk deploy --context environment=prod",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^20.10.0",
    "aws-cdk": "^2.170.0",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.170.0",
    "constructs": "^10.3.0",
    "fast-check": "^3.15.0",
    "source-map-support": "^0.5.21"
  }
}
